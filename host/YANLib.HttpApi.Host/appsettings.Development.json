{
  "Auth": {
    "Username": "*",
    "Password": "*"
  },
  "ConnectionStrings": {
    "Default": "*"
  },
  "Redis": {
    "Configuration": "*"
  },
  "Elasticsearch": {
    "Indices": {
      "Developer": "*",
      "Certificate": "*"
    },
    "Url": "*",
    "Username": "*",
    "Password": "*"
  },
  "CAP": {
    "ConnectionString": "*",
    "DBName": "*",
    "DefaultGroupName": "*",
    "Kafka": {
      "Connections": {
        "Default": {
          "BootstrapServers": "*"
        }
      },
      "Username": "*",
      "Password": "*"
    }
  },
  "Azure": {
    "ServiceBus": {
      "Connections": {
        "Default": {
          "ConnectionString": "*"
        }
      }
    },
    "EventBus": {
      "ConnectionName": "*",
      "SubscriberName": "*",
      "TopicName": "*"
    }
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "*",
        "Port": 5672,
        "Username": "*",
        "Password": "*"
        //"VirtualHost": "*",
        //"Ssl": {
        //  "Enabled": true,
        //  "ServerName": "*"
        //}
      }
    },
    "EventBus": {
      "ClientName": "*",
      "ExchangeName": "*"
    }
  },
  "ElasticApm": {
    "ServiceName": "*",
    "SecretToken": "*",
    "ServerUrl": "*",
    "Environment": "*",
    "CaptureBody": "*",
    "LogLevel": "*"
  },
  "AWSS3": {
    "AccessKeyId": "*",
    "SecretAccessKey": "*",
    "BucketName": "*",
    "Region": "*",
    "ClientName": "*"
  },
  "RemoteServices": {
    "EcommerceApi": {
      "BaseUrl": "*"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            },
            {
              "Name": "File",
              "Args": {
                "path": "Logs/.log",
                "rollingInterval": "Hour",
                "encoding": "System.Text.Encoding::UTF8"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "Logs/.json",
                "rollingInterval": "Hour",
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
              }
            }
          ]
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "*",
          "connectionGlobalHeaders": "*",
          "indexFormat": "*",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "*"
        }
      }
    ]
  }
}
